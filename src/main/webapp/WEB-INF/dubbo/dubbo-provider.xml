<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">

    <!--&lt;!&ndash; 提供方应用信息，用于计算依赖关系 &ndash;&gt;-->
    <!--<dubbo:application name="dubbo_provider" />-->

    <!--&lt;!&ndash; 使用zookeeper注册中心暴露服务地址 -即zookeeper的所在服务器ip地址和端口号 &ndash;&gt;-->
    <!--<dubbo:registry id = "dubboRegistry1" address="zookeeper://127.0.0.1:2182" protocol = "zookeeper" />-->

    <!--&lt;!&ndash; 用dubbo协议在20880端口暴露服务 &ndash;&gt;-->
    <!--<dubbo:protocol name="dubbo" port="28083" />-->

    <!--<dubbo:monitor protocol="registry"/>-->

     <!--声明需要暴露的服务接口-->
    <bean id="MsgRcvReceiveService" class="org.dubboMsgRcvHandler.MsgReceiveServiceImpl"/>
    <dubbo:service interface="org.dubbo.service.ReceiveService" ref="MsgRcvReceiveService"
                   timeout="30000" version="1.0" scope="remote" loadbalance="roundrobin" retries="0"/>

    <dubbo:reference id="reqMsgService" interface="org.dubbo.service.ReceiveService" version="1.0" check="false"/>

</beans>